<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.dsathyan.qrcodetest.Map"
    android:background="@drawable/basement">

    <Button
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Art Room"
        android:id="@+id/button"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#00000000" />

    <Button
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:id="@+id/button17"
        android:layout_alignBottom="@+id/button"
        android:layout_toRightOf="@+id/button"
        android:layout_toEndOf="@+id/button"
        android:background="#00000000" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:text="CHESS ROOM"
        android:id="@+id/button18"
        android:layout_alignTop="@+id/button17"
        android:layout_toRightOf="@+id/button17"
        android:layout_toEndOf="@+id/button17"
        android:background="#00000000" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="230dp"
        android:layout_height="50dp"
        android:id="@+id/button19"
        android:layout_alignTop="@+id/button"
        android:layout_toRightOf="@+id/button18"
        android:layout_toEndOf="@+id/button18"
        android:background="#00000000"/>

    <Button
        android:layout_width="440dp"
        android:layout_height="50dp"
        android:text="ART CHAMBER"
        android:id="@+id/button20"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="60dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="30dp"
        android:layout_above="@+id/button17"
        android:background="#00000000"/>

    <Button
        android:layout_width="50dp"
        android:layout_height="85dp"
        android:id="@+id/button21"
        android:layout_toRightOf="@+id/button20"
        android:layout_toEndOf="@+id/button20"
        android:layout_alignBottom="@+id/button20"
        android:background="#00000000"/>

    <Button
        android:layout_width="50dp"
        android:layout_height="150dp"
        android:text="DANCE ROOM"
        android:id="@+id/button22"
        android:layout_alignTop="@+id/button20"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#00000000"/>

    <Button
        android:layout_width="280dp"
        android:layout_height="50dp"
        android:text="MINI AUDITORIUM"
        android:id="@+id/button23"
        android:layout_above="@+id/button22"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="#00000000"/>

    <Button
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:text="AV ROOM"
        android:id="@+id/button24"
        android:layout_alignBottom="@+id/button23"
        android:layout_toRightOf="@+id/button23"
        android:layout_toEndOf="@+id/button23"
        android:background="#00000000"/>

    <Button
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:id="@+id/button25"
        android:layout_alignBottom="@+id/button24"
        android:layout_toRightOf="@+id/button24"
        android:layout_toEndOf="@+id/button24"
        android:background="#00000000"/>

    <Button
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:id="@+id/button26"
        android:layout_below="@+id/button25"
        android:layout_toRightOf="@+id/button20"
        android:layout_toEndOf="@+id/button20"
        android:background="#00000000"/>
</RelativeLayout>
